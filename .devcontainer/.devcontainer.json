{
    "name": "Adyen .NET 6 Online Payments Example",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp"
            ]
        }
    },
    "forwardPorts": [
        8080,
        5001
    ],
    "postCreateCommand": "dotnet --version && dotnet restore adyen-dotnet-online-payments.sln",
    "remoteEnv": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_MULTILEVEL_LOOKUP": "0"
    },
    "hostRequirements": {
        "cpus": 4
    },
    "portsAttributes": {
        "8080": {
            "label": "HTTP",
            "onAutoForward": "notify"
        },
        "5001": {
            "label": "HTTPS",
            "onAutoForward": "notify"
        }
    }
}